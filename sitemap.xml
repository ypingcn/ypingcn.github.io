---
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

<url>
  <loc>{{ site.url }}</loc>
  <lastmod>2020-11-01T12:00+08:00</lastmod>
  <changefreq>daily</changefreq>
  <priority>1.00</priority>
</url>
<url>
  <loc>{{ site.url }}/tags/</loc>
  <lastmod>2020-11-01T12:00+08:00</lastmod>
  <changefreq>daily</changefreq>
  <priority>1.00</priority>
</url>
<url>
  <loc>{{ site.url }}/notes/</loc>
  <lastmod>2020-11-01T12:00+08:00</lastmod>
  <changefreq>daily</changefreq>
  <priority>1.00</priority>
</url>
<url>
  <loc>{{ site.url }}/wiki/</loc>
  <lastmod>2020-11-21T11:30+08:00</lastmod>
  <changefreq>daily</changefreq>
  <priority>1.00</priority>
</url>

{% for post in site.posts %}
    <url>
      <loc>
      {{ site.url }}{{ post.url | remove: 'index.html' }}
      </loc>
      <lastmod>
      {% if post.update %}{{ post.update | date_to_xmlschema }}{% else %}{{ post.date | date_to_xmlschema }}{% endif %}
      </lastmod>
      <changefreq>daily</changefreq>
      <priority>1.00</priority>
    </url>
{% endfor %}
</urlset>